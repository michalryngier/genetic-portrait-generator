{
  "name": "@george_freeman/genetic-portrait-generator",
  "version": "1.0.0",
  "description": "Genetic Portrait Generator library",
  "scripts": {
    "build": "npx tsc",
    "autogypi": "autogypi",
    "node-gyp": "node-gyp",
    "emcc-path": "emcc-path",
    "copyasm": "copyasm",
    "ndts": "ndts",
    "build:native": "autogypi -r build && node-gyp -C++ build/native configure build && copyasm build/native dist",
    "build:asmjs":  "autogypi -r build && node-gyp -C++ build/asmjs  configure build --asmjs=1 && copyasm build/asmjs dist"
  },
  "author": "Michal Ryngier",
  "scope": "george_freeman",
  "license": "ISC",
  "main": "./dist/index.js",
  "devDependencies": {
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/jimp": "^0.2.28",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.13",
    "autogypi": "^0.2.2",
    "fs": "^0.0.1-security",
    "jimp": "^0.16.2",
    "lodash": "^4.17.21",
    "nbind": "^0.3.15",
    "node-gyp": "^10.0.1"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgeFr33man/genetic-portrait-generator.git"
  },
  "keywords": [
    "genetic",
    "portrait",
    "generator",
    "algorithm",
    "evolution",
    "evolutionary"
  ],
  "bugs": {
    "url": "https://github.com/georgeFr33man/genetic-portrait-generator/issues"
  },
  "homepage": "https://github.com/georgeFr33man/genetic-portrait-generator#readme"
}
